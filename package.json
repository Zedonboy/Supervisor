{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron ./dist/main.js",
    "build:watch": "node_modules/.bin/webpack --watch --progress",
    "build:dev": "node_modules/.bin/webpack --env.production false",
